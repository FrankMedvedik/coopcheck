<UserControl xmlns:vouchers="clr-namespace:muiCoopCheck.Pages.Vouchers"  x:Class="muiCoopCheck.Pages.Vouchers.EditBatchPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
            <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer>
            <Grid >
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <TextBlock> EDIT EXISTING BATCH</TextBlock>
                <Button x:Name="btnDOIT" IsEnabled="{Binding CanProceed}" Click="Button_Click" Grid.Row="1">Done</Button>
            </Grid>
        </ScrollViewer>
    </Grid>
</UserControl>
