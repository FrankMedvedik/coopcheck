<UserControl x:Class="muiCoopCheck.Pages.Batch.Import.ValidateView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="742.537">
    <Grid Style="{StaticResource ContentRoot}" >
        <ScrollViewer>
            <DataGrid AutoGenerateColumns="True" IsReadOnly="True" HorizontalAlignment="Left" Name="dgVoucherImports" VerticalAlignment="Top" 
                      ItemsSource="{Binding VoucherImports}" HorizontalScrollBarVisibility="Auto" Height="Auto" VerticalScrollBarVisibility="Auto" >
            </DataGrid>
        </ScrollViewer>
    </Grid>
</UserControl>
