<UserControl x:Class="CoopCheck.WPF.Content.ReportPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <mui:ModernTab Grid.Row="0" Layout="List" >
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Payment Finder" Source="/Content/Report/PaymentFinderReportPage.xaml" />
                <mui:Link DisplayName="Checks by Batch" Source="/Content/Report/BatchReportPage.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>


