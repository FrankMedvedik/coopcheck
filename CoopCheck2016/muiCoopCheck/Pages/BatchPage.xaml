<UserControl x:Class="muiCoopCheck.Pages.BatchPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Grid.Row="0" Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="new" Source="/Pages/Batch/NewBatchPage.xaml" />
                <mui:Link DisplayName="edit" Source="/Pages/Batch/EditBatchPage.xaml" />
                <mui:Link DisplayName="import" Source="/Pages/Batch/ImportPage.xaml" />
                <mui:Link DisplayName="print checks" Source="/Pages/Batch/PrintChecksPage.xaml" />
                <mui:Link DisplayName="swift pay" Source="/Pages/Batch/SwiftPayPage.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
