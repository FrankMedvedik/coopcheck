<UserControl x:Class="muiCoopCheck.Pages.PositivePayPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
       <mui:ModernTab Grid.Row="0" Layout="List">
                    <mui:ModernTab.Links>
                     <mui:Link DisplayName="new" Source="/Pages/PositivePay/NewPositivePayPage.xaml" />
                <mui:Link DisplayName="edit" Source="/Pages/PositivePay/OpenPositivePayPage.xaml" />
                <mui:Link DisplayName="submit" Source="/Pages/PositivePay/SubmitPositivePayPage.xaml" />
                    </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
